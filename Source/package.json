{
  "name": "polutiondetectionsystem",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "jsdoc ./ -r -d \"./../docs\" -c \"jsdoc.json\" -R \"./../README.md\"",
    "postgenerate": "glob-run html-beautify -r ./../docs/**/*.html -n -t -m 1 & glob-run css-beautify -r ./../docs/**/*.css -n -N -t",
    "lint": "eslint ./**/*.js",
    "lint-fix": "eslint ./**/*.js --fix",
    "postlint-fix": "glob-run css-beautify -r ./public/**/*.css -n -N -t",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "fast-haversine": "1.0.0",
    "hbs": "4.0.1",
    "jimp": "0.2.28",
    "mongoose": "5.0.13",
    "morgan": "1.9.0",
    "node-base64-image": "1.0.4",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "glob-run": "0.1.6",
    "js-beautify": "1.7.5",
    "jsdoc": "3.5.5",
    "jsdoc-http-plugin": "0.3.0"
  }
}
