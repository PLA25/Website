{
  "name": "polutiondetectionsystem",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "jsdoc ./ -r -d \"./../docs\" -c \"jsdoc.json\" -R \"./../README.md\"",
    "postgenerate": "glob-run html-beautify -r ./../docs/**/*.html -n -t -m 1 & glob-run css-beautify -r ./../docs/**/*.css -n -N -t & glob-run css-beautify -r ./public/**/*.css -n -N -t",
    "lint": "eslint ./**/*.js",
    "lint-fix": "eslint ./**/*.js --fix",
    "start": "node ./bin/www",
    "test": "nyc --reporter=html --reporter=text mocha --exit",
    "coverage-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage-codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "precommit": "npm run lint-fix"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "fast-haversine": "1.0.0",
    "hbs": "4.0.1",
    "i18n": "0.8.3",
    "jimp": "0.2.28",
    "mongoose": "5.0.14",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "request": "2.85.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.0",
    "coveralls": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "glob-run": "0.1.6",
    "husky": "0.14.3",
    "js-beautify": "1.7.5",
    "jsdoc": "3.5.5",
    "jsdoc-http-plugin": "0.3.0",
    "mocha": "5.1.0",
    "nyc": "11.6.0",
    "supertest": "3.0.0"
  }
}
