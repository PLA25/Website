<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <Document>
    {{#each sensorHubs}}
      <Placemark>
        <name>{{this.SerialID}}{{#if this.weight}}, {{this.weight}}{{/if}}</name>
        <Point>
          <coordinates>{{this.Longitude}}, {{this.Latitude}}, 0</coordinates>
        </Point>
      </Placemark>
    {{/each}}
  </Document>
</kml>
